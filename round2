vaishnavi pandey  Binary Search  //1,2,3,4,5,6,7 --> arr // val -> 6 Int binarySearch(vector<int> arr, val){ Left =0, right=n-1 While(left<=right){ 	Mid = (left+right)/2; 	If(arr[mid]==val) return mid; 	Else if(arr[mid]<val) left=mid+1; 	Else right=mid-1; } Returrn â€“1;  } Int main(){ 	Int n; 	Cin>>n; 	Vector<int> arr; 	For(int I=0;I<n;I++){ 		int val; 		cin>>val; 		arr.push_back(val); 	} 	Int searchele; 	Cin>> searchele; 	Cout<<binarySearch(arr,val); Return 0; }   			  		  		  1 4 3 2 8 7 6 5 12, 11, 10, 9	   Void traversal(Node* root){ 	Queue<Node*> q; 	If(root==nullptr) return; 	Q.push(root); 	While(q.size()>0)){ 		vector<int> vec; 		 For(int I=0;I<size();I++){ 		Node* temp = q.front(); 		vec.push_back(temp->val); 		int size = q.size(); 	q.pop(); 	If(temp->right) q.push(temp->right); 	if(temp->left) q.push(temp->left); } //print the list } 
